.Menu-list
  margin-bottom: 0
  padding-left: 0
  border-bottom: 1px solid #dfdfdf
  list-style-type: none

.Menu-item
  float: left
  width: 50%

.Menu-link
  display: block
  padding: 10px
  background-color: #f4f6f6
  text-align: center

.Menu-link:hover
  background-color: #fff

/* Hide in PDF version */
@media print
  .Menu
    display: none
