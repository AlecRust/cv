{
  "name": "cv",
  "version": "0.1.0",
  "description": "CV for Alec Rust in JSON Resume format.",
  "author": "Alec Rust <me@alecrust.com> (https://www.alecrust.com/)",
  "license": "MIT",
  "repository": "AlecRust/cv",
  "scripts": {
    "start": "resume serve --resume src/resume.json --theme .",
    "build": "gulp build",
    "test": "cd src && resume validate"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "date-fns": "^2.30.0",
    "gulp": "^4.0.2",
    "gulp-html-minifier": "^0.1.8",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "handlebars": "^4.7.7",
    "normalize.css": "^8.0.1",
    "postcss-custom-media": "^9.1.4",
    "postcss-custom-properties": "^13.2.0",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-reporter": "^7.0.5",
    "postcss-short-size": "^4.0.0",
    "postcss-sorting": "^8.0.2",
    "resume-cli": "^3.0.8",
    "sugarss": "^4.0.1"
  },
  "browserslist": [
    "last 2 versions",
    ">1%"
  ],
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  }
}
